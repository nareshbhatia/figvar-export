{
  "name": "figvar-export",
  "description": "A plugin to export Figma variables in W3C Design Tokens format",
  "version": "1.0.0",
  "author": "Naresh Bhatia",
  "license": "MIT",
  "homepage": "https://github.com/nareshbhatia/figvar-export",
  "repository": {
    "type": "git",
    "url": "https://github.com/nareshbhatia/figvar-export"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "clean": "rimraf build manifest.json node_modules",
    "fix": "run-s _lint:fix _format:fix",
    "lint": "run-s _lint _format",
    "watch": "build-figma-plugin --typecheck --watch",
    "_format": "prettier --list-different '*.{js,ts,json,md}' 'src/**/*.{css,js,jsx,ts,tsx,json,md}'",
    "_format:fix": "npm run _format -- --write",
    "_lint": "echo 'Linting is not implemented yet'",
    "_lint:fix": "echo 'Linting is not implemented yet'"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.0",
    "@create-figma-plugin/utilities": "^4.0.0",
    "jszip": "^3.10.1",
    "preact": "^10.26.5"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.0",
    "@create-figma-plugin/tsconfig": "^4.0.0",
    "@figma/plugin-typings": "1.110.0",
    "@types/node": "^22.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "figvar-export",
    "name": "Figvar Export",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
